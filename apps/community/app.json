{
  "expo": {
    "version": "1.0.0",
    "name": "Communiful",
    "slug": "sunrise-ui",
    "orientation": "portrait",
    "icon": "src/assets/images/app-icon.png",
    "scheme": "joincommuniful",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "src/assets/images/icon-transparent.png",
      "backgroundColor": "#200A3E"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "rsvp.sunrise.app",
      "supportsTablet": true,
      "bitcode": false,
      "associatedDomains": ["applinks:app.joincommuniful.com"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/icon-transparent.png",
        "backgroundColor": "#200A3E"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.SYSTEM_ALERT_WINDOW",
        "android.permission.WAKE_LOCK",
        "android.permission.BLUETOOTH",
        "com.android.vending.BILLING"
      ],
      "package": "rsvp.sunrise.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "app.joincommuniful.com"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/CourierPrime.ttf",
            "./src/assets/fonts/CourierPrimeBold.ttf",
            "./src/assets/fonts/Poppins.ttf",
            "./src/assets/fonts/PoppinsBold.ttf",
            "./src/assets/fonts/PoppinsLight.ttf"
          ]
        }
      ],
      "@config-plugins/react-native-webrtc",
      "@heap/react-native-heap",
      "expo-router",
      "@livekit/react-native-expo-plugin",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#200A3E",
          "image": "./src/assets/images/icon-transparent.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Communiful accesses photos to allow users to select a profile picture."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "owner": "devsunrisersvp-org",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
