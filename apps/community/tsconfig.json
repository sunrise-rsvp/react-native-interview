{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "esnext",
    "baseUrl": "src",
    "strict": true,
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@contexts/*": [
        "contexts/*"
      ],
      "@queries/*": [
        "queries/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@atoms/*": [
        "components/atoms/*"
      ],
      "@molecules/*": [
        "components/molecules/*"
      ],
      "@organisms/*": [
        "components/organisms/*"
      ],
      "@templates/*": [
        "components/templates/*"
      ],
      "@hoc/*": [
        "hoc/*"
      ]
    },
    "moduleSuffixes": [
      ".ios",
      ".android",
      ".native",
      ".web",
      ""
    ]
  },
  "include": [
    "src"
  ]
}
